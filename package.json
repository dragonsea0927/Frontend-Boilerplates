{
  "name": "myapp",
  "version": "2.0.0",
  "scripts": {
    "prod": "gulp --env=production",
    "postprod": "webpack --env.NODE_ENV=production",
    "dev": "gulp --env=development",
    "postdev": "webpack --env.NODE_ENV=development",
    "local": "gulp --env=local",
    "postlocal": "webpack --env.NODE_ENV=local",
    "start": "npm run local",
    "poststart": "concurrently \"gulp watcher --env=local\" \"webpack --env.NODE_ENV=local --watch\" --names \"GULP,PACK\" --prefix name"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "concurrently": "^3.5.0",
    "del": "^2.2.2",
    "eslint-loader": "^1.8.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-connect": "^5.0.0",
    "gulp-environment": "^1.3.0",
    "gulp-eslint": "^3.0.1",
    "gulp-fontmin": "^0.7.4",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-newer": "^1.3.0",
    "gulp-noop": "^1.0.0",
    "gulp-sass": "^2.3.2",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "opn": "^4.0.2",
    "require.all": "^2.0.3",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "normalize-scss": "^6.0.0"
  },
  "description": "Boilerplate with Gulp + SASS and ES6",
  "repository": "https://github.com/CKGrafico/Gulp-Boilerplates",
  "author": "Quique Fdez Guerra @CKGrafico"
}
